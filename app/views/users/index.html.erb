<p style="color: green"><%= notice %></p>

<h1>Psers</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="users">
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>

        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>

      </tr>
    <% end %>
  </tbody>
</table>




